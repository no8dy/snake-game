#include <list>
#include <iostream>

typedef void (*fp)();

void func1() {
	std::cout << "func1" << std::endl;
}
void func2() {
	std::cout << "func2" << std::endl;
}

int main() {

	std::list<fp> fpl;

	fpl.push_back(&func1);
	fpl.push_back(&func2);
	for ( std::list<fp>::iterator i = fpl.begin(); i != fpl.end(); ++i )
	   	(*i)();
}
